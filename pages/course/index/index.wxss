page {
  --height: 44px;
  --right: 190rpx;
  background-color: #f7f7f7;
}

.container-page {
}

.navigation-bar {
  overflow: hidden;
}

.navigation-bar .android {
  --height: 48px;
  --right: 222rpx;
}

.navigation-bar__inner {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5001;
  height: var(--height);
  display: flex;
  align-items: center;
  padding-right: var(--right);
  width: calc(100% - var(--right));
}

.navigation-bar__inner .navigation-bar__left {
  position: relative;
  width: 120;
  padding-left: 16px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  -webkit-box-pack: center;
}

.navigation-bar__inner .navigation-bar__left .navigation-bar__btn {
  display: inline-block;
  vertical-align: middle;
  background-repeat: no-repeat;
}

.navigation-bar__inner .navigation-bar__left .navigation-bar__btn_goback {
  font-size: 12px;
  width: 36rpx;
  height: 36rpx;
  background-image: url("data:image/png;base64,");
  /* background-position: 50% 50%; */
  background-size: cover;
}

.navigation-bar__inner .navigation-bar__left .navigation-bar__btn_goback:active {
  opacity: 0.5;
}

.navigation-bar__inner .navigation-bar__center {
  font-size: 17px;
  text-align: center;
  position: relative;
  flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.navigation-bar__inner .navigation-bar__right {
  margin-right: 16px;
}

.navigation-bar__placeholder {
  height: var(--height);
  background: #f8f8f8;
  position: relative;
  z-index: 50;
}

.navigation-bar-menu {
  width: 174rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 10rpx 24rpx 0rpx rgba(15, 22, 15, 0.22);
  border-radius: 10rpx;
  position: relative;
  top: 195rpx;
  left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.navigation-bar-menu .bar-menu-button {
