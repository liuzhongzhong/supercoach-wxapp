/**app.wxss**/

/* @import './components/weui/weui-wxss/dist/style/weui.wxss'; */

page {
  background-color: #f7f7f7;
}

.container {
  display: flex;
  flex-direction: column;
}

.container-page {
  display: flex;
  flex-direction: column;
}

.default-avatar {
  width: 84rpx;
  height: 84rpx;
  background: #2b87ff;
  border-radius: 50%;
  font-size: 24rpx;
  font-weight: 500;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

.select-cells-group .select-cells {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
}

.select-cells-group .select-cells .icon {
  width: 30rpx;
  height: 100%;
}

.select-cells-group .select-cells .content {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 720rpx;
  height: 100%;
  border-bottom: 1rpx solid #eff0f1;
}

.select-cells-group .select-cells .no-border {
  border: 0;
}

.select-cells-group .select-cells .content .title {
  width: 640rpx;
  height: 100%;
  display: flex;
  align-items: center;
  color: #181d24;
  font-size: 34rpx;
}

.select-cells-group .select-cells .content .arrow {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.select-cells-group .select-cells .content .arrow image {
  width: 14rpx;
  height: 24rpx;
}

.select-cells-group .select-cells-group {
  display: flex;
  flex-direction: column;
}

.no-border {
  border: 0rpx;
}

.common-bottom {
  width: 750rpx;
  height: 100rpx;
}

.fixed-bottom-button {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  background-color: #5fcd64;
}

.fixed-bottom-button button {
  background-color: #5fcd64;
  color: #fff;
  border-radius: 0rpx;
  border: 0rpx;
  font-size: 34rpx;
  font-weight: 500;
  width: 750rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.fixed-bottom-button button::after {
  border-radius: 0rpx;
  border: 0rpx;
  bottom: 0;
}

.movable-area, .movable-view {
  width: 750rpx;
  height: 120rpx;
}

.touch-item {
  display: flex;
  overflow: hidden;
}

.touch-move-active .select-cells, .touch-move-active .delete-button {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.delete-button {
  background-color: #ff3d3d;
  width: 120rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(120rpx);
  transform: translateX(120rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  font-size: 26rpx;
  font-weight: bold;
}

.select-cells {
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(120rpx);
  transform: translateX(120rpx);
  margin-left: -120rpx;
}

.input-cell-group {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  width: 750rpx;
}

.input-cell-group-margin {
  margin-top: 32rpx;
}

.input-cell-group .input-cell {
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.input-cell-group .input-cell .icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 28rpx;
  margin-left: 30rpx;
  margin-right: 24rpx;
}

.input-cell-group .input-cell .icon image {
  width: 28rpx;
  height: 28rpx;
}

.input-cell-group .input-cell .content {
  display: flex;
  align-items: center;
  width: 668rpx;
}

.input-cell-group .input-cell .content .title {
  font-size: 34rpx;
  color: #181d24;
  width: 168rpx;
}

.input-cell-group .input-cell .content .value {
  width: 470rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.input-cell-group .input-cell .content .value input {
  width: 470rpx;
  font-size: 34rpx;
  text-align: right;
}

.input-cell-group .input-cell .content .value text {
  font-size: 34rpx;
  color: #cfcfcf;
}

.input-cell-group .input-cell .content .value .hasValue {
  font-size: 34rpx;
  color: #181d24;
}

.input-cell-group .input-cell .content .value image {
  width: 14rpx;
  height: 24rpx;
  margin-left: 16rpx;
}

.input-cell-group .input-cell-border {
  border-bottom: solid 1rpx #eff0f1;
}

.input-placeholder {
  font-size: 34rpx;
  color: #cfcfcf;
}

.textarea-cell-group {
  display: flex;
  width: 100%;
  background-color: #fff;
  margin-top: 32rpx;
}

.textarea-cell {
  margin-top: 40rpx;
  margin-left: 30rpx;
  width: 690rpx;
}

.textarea-cell .title {
  display: flex;
  align-items: center;
  width: 100%;
}

.textarea-cell .title .icon {
  width: 28rpx;
  height: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.textarea-cell .title .icon image {
  width: 28rpx;
  height: 28rpx;
}

.textarea-cell .title .name {
  font-size: 34rpx;
  color: #181d24;
  margin-left: 24rpx;
}

.textarea-cell .input {
  width: 100%;
  height: 215rpx;
  margin-top: 40rpx;
  margin-bottom: 55rpx;
}

.textarea-cell .input textarea {
  font-size: 34rpx;
  width: 100%;
  height: 215rpx;
}

.textarea-placeholder {
  font-size: 34rpx;
  color: #cfcfcf;
}

.container-input {
  margin-top: 24rpx;
  display: flex;
  flex-direction: column;
}

.button-default {
  width: 690rpx;
  height: 88rpx;
  margin-left: 30rpx;
}

.button-default .disabled {
  background: #c7c7c7;
}

.button-default button {
  width: 690rpx;
  height: 88rpx;
  background: #5fcd64;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
  border: 0;
  margin-top: 48rpx;
}

.button-default button::after {
  border: 0;
}

.mongolia {
  width: 750rpx;
  height: 100%;
  background-color: #000;
  opacity: 0.8;
  position: fixed;
  top: 0;
  left: 0;
}

.button-line-list {
  background-color: #fff;
  width: 750rpx;
  display: flex;
  flex-direction: column;
  margin-bottom: 32rpx;
}

.button-line-list .button-line {
  background-color: #fff;
  width: 720rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 30rpx;
  height: 120rpx;
  border-bottom: 1rpx solid #eff0f1;
}

.button-line-list .no-border {
  border: 0;
}

.button-line-list .button-line .name {
  font-size: 34rpx;
  color: #181d24;
  width: 390rpx;
}

.button-line-list .button-line .more {
  font-size: 34rpx;
  color: #cfcfcf;
  width: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.button-line-list .button-line .more image {
  width: 14rpx;
  height: 24rpx;
}

.add-button {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
  position: fixed;
  right: 30rpx;
  bottom: 168rpx;
}

.add-button image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
}

.result-input-group {
  width: 750rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  margin-bottom: 32rpx;
}

.result-input-group .input-line {
  width: 720rpx;
  margin-left: 30rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #eff0f1;
}

.result-input-group .no-border {
  border: 0;
}

.result-input-group .input-line .name {
  width: 390rpx;
  display: flex;
  flex-direction: column;
  color: #181d24;
  font-size: 34rpx;
}

.result-input-group .input-line .value {
  width: 300rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.result-input-group .input-line .value input {
  width: 300rpx;
  display: flex;
  font-size: 34rpx;
  text-align: right;
  color: #181d24;
}

.result-input-group .input-line .value .input-placeholder {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 34rpx;
  color: #cfcfcf;
}

.result-input-group .input-line .value text {
  font-size: 34rpx;
  color: #cfcfcf;
}

.result-input-group .input-line .value .select-value {
  color: #181d24;
}

.result-input-group .input-line .value image {
  width: 14rpx;
  height: 24rpx;
  margin-left: 16rpx;
}

.add-action {
  margin-top: 104rpx;
}

.lesson-list {
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 45rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.lesson {
  display: flex;
  flex-direction: column;
  margin-bottom: 56rpx;
  margin-right: 30rpx;
}

.no-margin {
  margin-right: 0rpx;
}

.lesson .checked {
  width: 204rpx;
  height: 45rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
}

.lesson .checked radio {
  zoom: 0.8;
}

.lesson .background {
  width: 210rpx;
  height: 280rpx;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: -54rpx;
  background-color: #FFF;
}

.lesson .background image {
  width: 210rpx;
  height: 280rpx;
  border-radius: 12rpx;
}

.lesson .background .icon {
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
}

.lesson .background .icon image{
  width: 50rpx;
  height: 50rpx;
}

.lesson .background .name{
  color: #5FCD64;
  font-size: 28rpx;
  margin-top: 30rpx;
}

.lesson .title {
  width: 178rpx;
  height: 48rpx;
  border-radius: 0rpx 0rpx 12rpx 12rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  padding-left: 16rpx;
  padding-right: 16rpx;
  margin-top: -48rpx;
  z-index: 999;
}

.lesson .times {
  color: #707888;
  font-size: 24rpx;
  margin-top: 16rpx;
  width: 178rpx;
  margin-left: 16rpx;
}

.lesson .marks {
  width: 210rpx;
  height: 48rpx;
  background: #000;
  opacity: 0.4;
  border-radius: 0rpx 0rpx 12rpx 12rpx;
  margin-top: -48rpx;
}